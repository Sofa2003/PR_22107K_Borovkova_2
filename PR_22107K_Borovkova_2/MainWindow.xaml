<Window x:Class="PR_22107K_Borovkova_2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PR_22107K_Borovkova_2"
        mc:Ignorable="d"
        Title="Borovkova 2
        " Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="13*"/>
            <ColumnDefinition Width="147*"/>
        </Grid.ColumnDefinitions>
        <Button x:Name="btres" Content="Результат" HorizontalAlignment="Left" Margin="492,53,0,0" VerticalAlignment="Top" Height="49" Width="139" Grid.Column="1" Click="btres_Click"/>
        <TextBox x:Name="txvvod" Grid.Column="1" HorizontalAlignment="Left" Margin="239,62,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="219" Height="32" PreviewTextInput="txvvod_PreviewTextInput"/>
        <Label x:Name="lbvvod" Content="Введите строку-предожение на английском" HorizontalAlignment="Left" Margin="47,65,0,0" VerticalAlignment="Top" Grid.ColumnSpan="2" FontFamily="Segoe UI Symbol" Width="257"/>
        <ListBox x:Name="listres" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="32,160,42,13" Grid.ColumnSpan="2"/>

    </Grid>
</Window>
